<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="2dp"
    android:layout_marginHorizontal="5dp">

    <TextView
        android:id="@+id/textViewDataDespesa"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:text="26/02/2022 08:01"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textViewValorDespesa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toStartOf="@+id/textViewTipoDespesa"
        android:layout_toEndOf="@+id/textViewDataDespesa"
        android:layout_marginLeft="10dp"
        android:text="R$ 101,00"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textViewTipoDespesa"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Trampo de Freela Web"
        android:textSize="10sp"
        android:layout_marginLeft="60dp"
        android:layout_alignParentEnd="true" />

</LinearLayout>